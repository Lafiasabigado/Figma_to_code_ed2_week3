{
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ],
  "rewrites": [
    { "source": "/html/:path*", "destination": "/public/:path*" },
    { "source": "/css/:path*", "destination": "/src/assets/css/:path*" },
    { "source": "/js/:path*", "destination": "/src/assets/:path*" },
    { "source": "/images/:path*", "destination": "/src/assets/images/:path*" }
  ]
}
